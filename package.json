      "express-fileupload": "^1.5.1",
      "firebase-admin": "^13.3.0",
      "multer": "1.4.5-lts.2",
      "pug": "^3.0.3",
      "rimraf": "^6.0.1",
      "sharp": "^0.34.1"
   },
   "husky": {
      "hooks": {
         "pre-commit": "npx pretty-quick --staged ng lint ng test",
         "pre-push": "ng build --aot true"
      }
   }
